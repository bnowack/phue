{
  "name": "phue-dev-tools",
  "version": "0.1.4",
  "description": "Dev tools for Phue",
  "files": [
    "package.json",
    "package-lock.json"
  ],
  "private": true,
  "devDependencies": {
    "chromedriver": "^2.33.1",
    "geckodriver": "^1.9.0",
    "jasmine-core": "*",
    "karma": "*",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "*",
    "karma-junit-reporter": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "phantomjs-prebuilt": "^2.1.15"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-regenerator-runtime": "^6.5.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "fs-extra": "^4.0.2",
    "moment": "^2.19.2",
    "native-promise-only": "^0.8.1",
    "node-sass": "^4.7.2",
    "qs": "^6.5.1",
    "sass-loader": "^6.0.6",
    "vue": "^2.5.8",
    "vue-cli": "^2.9.1",
    "vue-loader": "^13.5.0",
    "vue-material": "1.0.0-beta-6",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.8",
    "webpack": "^3.8.1",
    "webpack-clean-obsolete-chunks": "^0.3.0"
  },
  "scripts": {
    "install-fresh": "rm -rf ./node_modules && npm install -o",
    "app": "php -S 0.0.0.0:8888 dev/index.php",
    "selenium-chrome-mac": "java -Dwebdriver.chrome.driver=node_modules/chromedriver/bin/chromedriver -jar vendor/se/selenium-server-standalone/bin/selenium-server-standalone.jar || true",
    "selenium-chrome-win": "java -Dwebdriver.chrome.driver=node_modules\\.bin\\chromedriver.cmd -jar vendor\\se\\selenium-server-standalone\\bin\\selenium-server-standalone.jar || true",
    "build-dev": "webpack --watch --config webpack.config.dev.js --colors --progress --hide-modules || true",
    "build-stats": "webpack --config webpack.config.prod.js --json > webpack-stats.json",
    "build-prod": "webpack --config webpack.config.prod.js --colors --progress --hide-modules || true",
    "js-tests": "karma start test/unit/js/karma.conf.js -- --tags @active"
  }
}
